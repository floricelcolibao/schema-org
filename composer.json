{
    "name": "spatie/schema-org",
    "description": "A fluent builder Schema.org types and ld+json generator",
    "keywords": [
        "spatie",
        "schema-org"
    ],
    "homepage": "https://github.com/spatie/schema-org",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6"
    },
    "require-dev": {
        "larapack/dd": "^1.1",
        "league/flysystem": "^1.0",
        "phpunit/phpunit": "^3.7",
        "scrutinizer/ocular": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\SchemaOrg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\SchemaOrg\\Generator\\": "generator",
            "Spatie\\SchemaOrg\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "generate": "php ./generate.php generate"
    },
    "config": {
        "sort-packages": true
    }
}
